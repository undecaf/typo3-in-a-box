{
	"name": "undecaf/typo3-in-a-box",
	"description" : "Containerized TYPO3 8.7 in Composer mode",
    "license": "GPL-2.0+",
    "repositories": [
        { "type": "composer", "url": "https://composer.typo3.org/" }
    ],
	"require": {
		"typo3/cms": "^8.7"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    }
}
